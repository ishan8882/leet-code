import java.util.Arrays;

public class test {
    public static void main(String[] args) {
        int[] arr = { 1, 3, 5, 6 };
        System.out.println(Arrays.toString(arr));
        int value = 3;
        int count = 0;
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == value)
                count = i;
        }
        System.out.println(value + " pesent at " +count + " index ");
    }
}
